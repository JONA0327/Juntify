# --- Audio Conversion / FFmpeg ---
# Fuerza la conversión a OGG (Opus)
AUDIO_FORCE_OGG=true
# Bitrate de Opus (64k, 96k, 128k, etc.)
AUDIO_OPUS_BITRATE=96k
# Timeout en segundos para ffmpeg/convertidores
AUDIO_CONVERSION_TIMEOUT=1800

# Usa el script de Python para convertir (recomendado en servidores Linux)
# true/false
AUDIO_USE_PYTHON=false
# Binario de Python (python3 en Linux, python en Windows si corresponde)
PYTHON_BIN=python3

# Rutas de binarios de FFmpeg/FFprobe (si no están en el PATH)
FFMPEG_BIN=ffmpeg
FFPROBE_BIN=ffprobe

